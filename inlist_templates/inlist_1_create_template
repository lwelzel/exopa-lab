! inlist_create

&star_job

      show_log_description_at_start = .true.

      save_model_when_terminate = .true.
      save_model_filename = <<save_model_filename1>>
      
      ! new adiabatic, contracting initial model
      create_initial_model = .true.
      radius_in_cm_for_create_initial_model = 2.09733e10  !3 Rjup in cm
      mass_in_gm_for_create_initial_model = <<initial_mass>> !30 Mearth in grams
      initial_model_relax_num_steps = 0


      set_initial_age = .true.
      initial_age = 0.0 ! in years

      eos_file_prefix = 'mesa'
      kappa_file_prefix = 'gs98'
      kappa_lowT_prefix = 'lowT_Freedman11'

      change_lnPgas_flag = .true.
      new_lnPgas_flag = .true.
      
      !pgstar_flag = .true.
      pause_before_terminate = .false. ! lets you examine the final pgstar window when run terminates

/ !end of star_job

&controls

	! for create_initial_model
	initial_Z = 0.02
	initial_Y = 0.25

        varcontrol_target = 1d-4
 	mesh_delta_coeff = 0.5


	! when to stop
	max_age = 1.d3
        max_model_number = 1000         ! don't let it go on and on if run stuck
	T_mix_limit = 0

	! output to files and terminal
	history_interval = 2
	terminal_interval = 10


        star_history_name = <<star_history_name1>>
	log_directory = <<log_directory>>


/


&pgstar
         


      


/ ! end of pgstar namelist
